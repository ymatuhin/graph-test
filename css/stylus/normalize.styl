article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
main,
nav,
section,
summary
	display block

audio,
canvas,
video
	if !ie
		display inline-block
	if ie
		display inline
		zoom 1

audio
	&:not([controls])
		if !ie
			display none
			height 0

// 	Ð² helpers.styl
// 	[hidden]
// 		display none

html
	if ie
		font-size 100.01%
	if !ie
		-ms-text-size-adjust 100%
		-webkit-text-size-adjust 100%

html,
button,
input,
select,
textarea
	font-family sans-serif

body
	margin 0 if !ie

a
	&:focus
		outline thin dotted

a:active,
a:hover
	outline 0

h1
	font-size 2em
	margin 0.67em 0

h2
	font-size 1.5em
	margin 0.83em 0

h3
	font-size 1.17em
	margin 1em 0

h4
	font-size 1em
	margin 1.33em 0

h5
	font-size 0.83em
	margin 1.67em 0

h6
	font-size 0.67em
	margin 2.33em 0

abbr[title]
	border-bottom 1px dotted

b,
strong
	font-weight bold

blockquote
	margin 1em 40px

dfn
	font-style italic if !ie

hr
	if !ie
		-moz-box-sizing content-box
		box-sizing content-box
		height 0

mark
	background #ff0
	color #000

p,
pre
	margin 1em 0 if ie

code,
kbd,
pre,
samp
	font-family monospace, serif
	_font-family 'courier new', monospace
	font-size 1em

pre
	white-space pre
	white-space pre-wrap
	word-wrap break-word

q
	quotes none if ie

q:before,
q:after
	if !ie
		content ''
		content none

small
	font-size 80%

sub,
sup
	font-size 75%
	line-height 0
	position relative
	vertical-align baseline

sup
	top -0.5em

sub
	bottom -0.25em

dl,
menu,
ol,
ul
	margin 1em 0 if ie

dd
	margin 0 0 0 40px if ie

menu,
ol,
ul
	padding 0 0 0 40px if ie

nav ul,
nav ol
	if ie
		list-style none
		list-style-image none

img
	border 0
	-ms-interpolation-mode bicubic if ie

svg
	&:not(:root)
		overflow hidden if !ie

figure
	margin 0

form
	margin 0 if ie

fieldset
	border 1px solid #c0c0c0
	margin 0 2px
	padding 0.35em 0.625em 0.75em

legend
	border 0
	padding 0
	white-space normal if !ie
	margin-left -7px if ie

button,
input,
select,
textarea
	font-size 100%
	margin 0
	vertical-align baseline if !ie
	vertical-align middle if ie

button,
input
	line-height normal

button,
select
	text-transform none

button,
html input[type="button"],
input[type="reset"],
input[type="submit"]
	-webkit-appearance button if !ie
	cursor pointer
	overflow visible if ie

button[disabled],
html input[disabled]
	cursor default

input[type="checkbox"],
input[type="radio"]
	if !ie
		box-sizing border-box
		padding 0
	if ie
		height 13px
		width 13px

input[type="search"]
	if !ie
		-webkit-appearance textfield
		-moz-box-sizing content-box
		-webkit-box-sizing content-box
		box-sizing content-box

input[type="search"]::-webkit-search-cancel-button,
input[type="search"]::-webkit-search-decoration
	-webkit-appearance none if !ie

button::-moz-focus-inner,
input::-moz-focus-inner
	if !ie
		border 0
		padding 0

textarea
	overflow auto
	vertical-align top

table
	border-collapse collapse
	border-spacing 0

